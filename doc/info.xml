    <matlabrelease>R2019a</matlabrelease>
    <name>OpenCossan</name>
    <type>toolbox</type>
    <icon>none</icon>
    <help_location>/html</help_location>
    <help_contents_icon>none</help_contents_icon>
    
    <list>
        
<listitem>
<label>Help</label>
<callback>doc opencossan/</callback>
<icon>fullfile('images','OpenCossanIcon.png')</icon>
</listitem>
        
        <listitem>
            <!-- The label provides the text for this menu item --> 
            <label>Documentation</label>
            <!-- This callback is a command to open your documentation -->
            <callback>web(fullfile(OpenCossan.getCossanRoot,'doc','html','COSSAN_product_page.html'),'-helpbrowser')
            </callback>
            <!-- Menu item icon (a toolbox icon from the help browser ) -->
            <icon>$toolbox/matlab/icons/bookicon.gif</icon>
        </listitem>
        
        <listitem>
            <label>Demos </label>
            <callback>demo toolbox 'OpenCossan'</callback>
            <icon>ApplicationIcon.DEMOS</icon>
        </listitem>
        
        
        <listitem>
            <label>Help - User Manual (web)</label>
            <callback>web('http://www.cossan.co.uk/wiki/index.php/Category:User_Manual','-browser');</callback>
            <icon>$toolbox/matlab/icons/book_mat.gif</icon>
        </listitem>
        
        <listitem>
            <label>Help - Refernce Manual (web)</label>
            <callback>web('http://www.cossan.co.uk/wiki/index.php/Category:Reference_Manual','-browser');</callback>
            <icon>$toolbox/matlab/icons/book_mat.gif</icon>
        </listitem>
        
        <listitem>
            <label>Help - Theory Manual (web)</label>
            <callback>web('http://www.cossan.co.uk/wiki/index.php/Category:Theory_Manual','-browser');</callback>
            <icon>$toolbox/matlab/icons/book_mat.gif</icon>
        </listitem>
        
        <listitem>
            <label>Web Site (web) </label>
            <callback>web('https://www.cossan.co.uk','-browser');</callback>
            <icon>$docroot/techdoc/matlab_env/examples/webicon.gif</icon>
        </listitem>
        
    </list>
    
</productinfo>
